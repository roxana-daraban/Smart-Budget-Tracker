# Database Credentials
# Copy this file to application-secrets.properties and fill in your actual values
spring.datasource.username=root
spring.datasource.password=root

# JWT Secret Key (generate a strong random string, at least 256 bits)
# You can generate one using: openssl rand -base64 32
jwt.secret=YOUR_JWT_SECRET_KEY_HERE

# JWT Expiration (in milliseconds) - 24 hours default
jwt.expiration=86400000

# Google Gemini API Key (for AI Financial Advisor feature)
gemini.api.key=YOUR_GEMINI_API_KEY_HERE

# Currency Exchange API Key (optional)
currency.api.key=YOUR_CURRENCY_API_KEY_HERE